    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "canvas-confetti": "^1.9.3",
    "classnames": "^2.5.1",
    "cookie": "^1.0.2",
    "deepmerge": "^4.3.1",
    "dotenv-extended": "^2.9.0",
    "graphql": "^16.11.0",
    "http-proxy-middleware": "^3.0.5",
    "intl-messageformat": "^10.7.16",
    "lodash": "^4.17.21",
    "next": "^15.5.2",
    "react": "^19.1.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
