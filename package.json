    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-formatjs": "^5.4.2",
    "eslint-plugin-prettier": "^5.5.4",
    "graphql-config": "^5.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
