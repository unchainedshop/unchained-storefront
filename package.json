  "private": true,
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@apollo/client": "^4.0.4",
    "@coingecko/cryptoformat": "^0.8.2",
    "@formatjs/intl-datetimeformat": "^6.18.0",
